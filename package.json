{
  "name": "eslint-config-codestates",
  "version": "0.0.1",
  "private": false,
  "author": "fronttigger <fronttigger@gmail.com>",
  "license": "MIT",
  "description": "CodeStates's ESLint config",
  "main": "index.js",
  "files": [
    "rules",
    "index.js",
    "frontend.js",
    "prettierrc.js"
  ],
  "keywords": [
    "codestates",
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "typescript",
    "react",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "lint": "eslint './**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier '**/*.{json,yaml,md}' --check",
    "prettier:fix": "prettier '**/*.{json,yaml,md}' --write",
    "deploy": "npm publish",
    "deploy:canary": "sh ./scripts/canary-publish.sh",
    "version": "npm version"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.1"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "typescript": "^4.9.4"
  }
}
